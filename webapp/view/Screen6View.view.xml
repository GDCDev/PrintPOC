					        <Button xmlns="sap.m" text="{i18n>cancel}" width="100px" id="button0" press="action" xmlns:action="http://schemas.sap.com/sapui5/extension/sap.ui.core.CustomData/1" action:wiring="\{'press':\{'navigation':\{'routeName':'Screen1View'\}\}\}"/>
