<mvc:View xmlns:core="sap.ui.core" xmlns:mvc="sap.ui.core.mvc" xmlns="sap.m" xmlns:html="http://www.w3.org/1999/xhtml" controllerName="sap.m.PrintPOC.controller.Screen6View" xmlns:cd="http://schemas.sap.com/sapui5/extension/sap.ui.core.CustomData/1">
